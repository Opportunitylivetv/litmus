LISA s+srcu+rl-o-rul (*7-RCU*)
(* Forbid: At least as many GPs and RCU RS CSes. *)
{
x = 0;
y = 0;
}
 P0          | P1                 ;
             | f[rcu_read_lock]   ;
 w[once] y 2 | r[once] r1 x       ;
 f[sync]     |                    ;
 w[once] x 1 | w[once] y 1        ;
             | f[rcu_read_unlock] ;
exists (y=2 /\ 1:r1=1)

